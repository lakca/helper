{
  "source": {
    "include": [
      "index.js",
      "README.md"
    ]
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": true,
    "useLongnameInNav": true,
    "showInheritedInNav": true
  },
  "opts": {
    "destination": "./docs/",
    "template": "./node_modules/minami"
  }
}
